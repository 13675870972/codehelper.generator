<idea-plugin version="2">
    <id>com.ccnode.codegenerator.mybatis.generator</id>
    <name>cyc®generatorHelper</name>
    <version>1.0</version>
    <vendor email="414582939@qq.com" url="http://www.modengdasheng.com">CodeHelper</vendor>

    <description><![CDATA[
      <div>
        <p>Mybatis Mapper Helper 1.0.3</p>
