            <add-to-group group-id="ToolsMenu" relative-to-action="GenerateJavadoc" anchor="before"/>
            <action id="mybatis.AutoCoding" class="com.ccnode.codegenerator.view.AutoCodingAction" text="GenAllSetter"
                    description="GenAllSetter"/>
            <action id="mybatis.genMybatisCode" class="com.ccnode.codegenerator.view.GenCodeAction" text="GenDaoCode"
                    description="GenMybatisCode"/>
            <action id="mybatis.LearnMore" class="com.ccnode.codegenerator.view.ShowLearnMoreAction" text="Learn More"
                    description="Learn_More"/>
            <!--<com.ccnode.codegenerator.action id="mybatis.enterLicense" class="com.ccnode.codegenerator.view.EnterLicenseAction" text="Register" description="License"/>-->

            <!--<com.ccnode.codegenerator.action id="mybatis.generator" class="com.ccnode.codegenerator.view.GenCodeAction" text="genCode" description="A test menu item"/>-->
        </group>
        <!--<group id="com.ccnode.codegenerator.mybatis.generator" text="Gen" description="CodeGenerator">-->
        <!--<add-to-group group-id="TestActionSubGroup" />-->
        <!--<com.ccnode.codegenerator.action id="mybatis.generator_1" class="com.ccnode.codegenerator.view.GenCodeAction" text="genCode1" description="A test menu item1"/>-->
        <!--<com.ccnode.codegenerator.action id="mybatis.generator_2" class="com.ccnode.codegenerator.view.GenCodeAction" text="genCode2" description="A test menu item2"/>-->

        <!--</group>-->
        <group popup="true" id="com.cyc.mybatis.generator" text="cyc工具箱" description="MyBatisTools">
            <action id="GenerateMapperAction" class="com.ccnode.codegenerator.action.GenerateApplication" text="Mybatis生成器"
                    description="GenerateMapperAction"/>
            <action id="GenerateXMLApplication" class="com.ccnode.codegenerator.action.GenerateXMLApplication" text="Config生成器"
                    description="GenerateMapperAction"/>
            <add-to-group group-id="ProjectViewPopupMenu" anchor="last"/>
            <add-to-group group-id="EditorPopupMenu" anchor="last"/>
            <add-to-group group-id="ChangesViewPopupMenu" anchor="last"/>
        </group>
    </actions>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
